{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently 'yarn docker:up' 'yarn start' 'yarn s:start'",
    "start": "react-scripts start",
    "s:start": "NODE_ENV=dev nodemon server/bin/www",
    "s:db:migrate": "knex migrate:latest",
    "s:test": "jest server",
    "s:test:db:create": "node server/db/test/createDb.js",
    "s:test:db:drop": "node server/db/test/dropDb.js",
    "s:test:db:migrate": "NODE_ENV=test knex migrate:latest",
    "s:test:db:reset": "yarn s:test:db:drop && yarn s:test:db:create && yarn s:test:db:migrate",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "docker:up": "docker-compose up"
  },
  "dependencies": {
    "bootstrap": "^4.2.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "http-proxy-middleware": "^0.19.1",
    "js-cookie": "^2.2.0",
    "jsonwebtoken": "^8.4.0",
    "knex": "^0.16.3",
    "lodash": "^4.17.11",
    "morgan": "~1.9.0",
    "objection": "^1.5.3",
    "objection-unique": "^1.0.1",
    "passport": "^0.4.0",
    "passport-facebook": "^3.0.0",
    "passport-local": "^1.0.0",
    "pg": "^7.8.0",
    "query-string": "^6.2.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
    "react-scripts": "2.1.3",
    "redux": "^4.0.1",
    "redux-first-router": "^2.1.1",
    "redux-first-router-link": "^2.1.1",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "eslint-config-prettier": "^3.6.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "nodemon": "^1.18.9",
    "prettier": "^1.16.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
